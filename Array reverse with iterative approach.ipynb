   "source": [
    "def reverse(arr, n):\n",
    "    start=0\n",
    "    end=n-1\n",
    "    \n",
    "    while start<end:\n",
    "        arr[start], arr[end] = arr[end], arr[start]\n",
    "        start=start+1\n",
    "        end=end-1\n",
    "\n",
    "arr = [1, 2, 3, 4, 5]\n",
    "reverse(arr, 5)\n",
    "print(arr)\n",
    "\n",
    "#Time Complexity: O(n) and Space Complexity: O(1)"
   ]
  }
