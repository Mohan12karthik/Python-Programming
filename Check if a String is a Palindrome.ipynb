   "source": [
    "def is_palindrome(str):\n",
    "    rev_str=str[::-1]\n",
    "    return str==rev_str\n",
    "\n",
    "word = \"madam\"\n",
    "if is_palindrome(word):\n",
    "    print(f\"{word} is a palindrome\")\n",
    "else:\n",
    "    print(f\"{word} is not a palindrome\")"
   ]
