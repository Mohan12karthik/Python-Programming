   "source": [
    "def full_pyramid(n):\n",
    "    for i in range(1,n+1):\n",
    "        for j in range(n-i):\n",
    "            print(\" \", end=\" \")\n",
    "        \n",
    "        for k in range(1, i*2):\n",
    "                print(\"*\", end=\" \")\n",
    "        print()\n",
    "\n",
    "full_pyramid(5)"
   ]
