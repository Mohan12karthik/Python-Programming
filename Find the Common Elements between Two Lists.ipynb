    "def find_common_elements(list1, list2):\n",
    "    common_el=[]\n",
    "    \n",
    "    for item in list1:\n",
    "        if item in list2:\n",
    "            common_el.append(item)\n",
    "    return common_el\n",
    "\n",
    "\n",
    "list_a = [1, 2, 3, 4, 5]\n",
    "list_b = [4, 5, 6, 7, 8]\n",
    "common = find_common_elements(list_a, list_b)\n",
    "print(common)"
