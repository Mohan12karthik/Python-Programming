   "source": [
    "def find_largest(numbers):\n",
    "    largest = numbers[0]\n",
    "    \n",
    "    for num in numbers:\n",
    "        if num > largest:\n",
    "            largest = num\n",
    "    return largest\n",
    "\n",
    "nums = [10, 5, 8, 20, 3]\n",
    "largest_number = find_largest(nums)\n",
    "print(f\"The largest number is {largest_number}\")"
   ]
